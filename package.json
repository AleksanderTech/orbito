{
  "name": "orbito",
  "version": "2.1.1",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    },
    "./lib/component": {
      "import": "./lib/component.mjs",
      "require": "./lib/component.js"
    },
    "./*": "./*"
  },
  "description": "Orbito is a Node.js site generator (SSG + SSR) that can write static HTML files or return server-rendered HTML, with a bundled client script when you need it",
  "scripts": {
    "build": "tsc && tsup src --format cjs,esm --dts --clean --outDir lib"
  },
  "keywords": [
    "SSG",
    "static site generator",
    "SSR",
    "server side rendering"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AleksanderTech/orbito.git"
  },
  "homepage": "https://www.npmjs.com/package/orbito",
  "author": "Aleksander RoztropiÅ„ski",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "esbuild": "^0.20.1"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
